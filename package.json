{
  "name": "todox",
  "version": "0.1.0",
  "description": "Pure Javascript Todolist with webpack and ES6+",
  "scripts": {
    "watch": "webpack -d --watch",
    "serve": "webpack-dev-server -d",
    "report": "webpack-bundle-analyzer --port 4200 dist/stats.json",
    "jserver": "json-server --watch todox.json --port 5555",
    "start": "concurrently \"npm:watch\" \"npm:serve\""
  },
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.5",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "animate.css": "^3.7.2",
    "bootstrap": "4.4.1",
    "loaders.css": "^0.1.2",
    "webpack": "^4.42.1",
    "concurrently": "5.1.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "babel-loader": "8.1.0",
    "bootbox": "^5.4.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "3.5.2",
    "file-loader": "6.0.0",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.2.0",
    "jquery": "3.5.0",
    "json-server": "^0.16.1",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.4",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
